  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Eos Thesis IDE",
        "preferences": {
          "toolbar.showToolbar": true
        }
      }
    },
    "backend": {
      "config": {
        "startupTimeout": -1,
        "resolveSystemPlugins": false
      }
    },
    "generator": {
      "config": {
        "preloadTemplate": "./resources/preload.html"
      }
    }
  },
